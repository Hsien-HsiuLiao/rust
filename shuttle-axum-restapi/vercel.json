{
  "buildCommand": "source $HOME/.cargo/env && cargo build --release",
  "outputDirectory": "target/release",
  "installCommand": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y && source $HOME/.cargo/env && rustup default stable",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "env": {
    "RUST_BACKTRACE": "1"
  },
  "build": {
    "env": {
      "RUST_BACKTRACE": "1"
    }
  }
} 